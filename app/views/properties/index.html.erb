<h1>Listing <%= @property_type %>s</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% for property in @properties %>
  <tr>
    <td><%= property.name %></td>
    <td><%= link_to 'Edit', :action=>"edit", :id=>property.id %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to "New #{@property_type}", :action=>"new" %>
