  <p>
    <%= f.label :effective_at %><br />
    <%= calendar_box f.object_name, :effective_at %>
  </p>
  <p>
    <%= f.label :ineffective_at %><br />
    <%= calendar_box f.object_name, :ineffective_at %>
  </p>
  <p>
    <%= f.label :weekday_id %><br />
    <%= select(f.object_name, "weekday_id", Weekday.find(:all).sort_by(&:id).collect{|p| [p.name, p.id]}) %>
  </p>
  <p>
    <%= f.label :start_time %><br />
    <%= f.time_select :start_time %>
  </p>
  <p>
    <%= f.label :end_time %><br />
    <%= f.time_select :end_time %>
  </p>
  <p>
    <%= f.label :slot_count %><br />
    <%= f.text_field :slot_count %>
  </p>
  <p>
    <%= f.label :volunteer_task_type_id %><br />
    <%= select(f.object_name, "volunteer_task_type_id", VolunteerTaskType.find(:all).sort_by(&:name).collect {|p| [ p.description, p.id ] }) %>
  </p>
  <p>
    <%= f.label :roster_id %><br />
    <%= select(f.object_name, "roster_id", Roster.find(:all).sort_by(&:name).collect{|p| [ p.name, p.id ]}) %>
  </p>
