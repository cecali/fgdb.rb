<h1>Editing pricing type</h1>

<% form_for(@pricing_type, :url => {:action => "update", :id => @pricing_type.id}) do |f| %>
  <%= f.error_messages %>

  <%= render :partial => "form", :locals => {:f => f} %>

  <p style="clear: left">
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Back', {:action => "index"} %>
