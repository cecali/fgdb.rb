<h2>Showing pricing table for <%= @table_name %></h2>

<table class="awesome">
  <% @table.each_with_index do |row, i| %>
  <tr>
    <% row.each do |x| %>
    <% if i == 0 %>
      <th><%= x %></th>
    <% else %>
      <td><%= x %></td>
    <% end %>
    <% end %>
  </tr>
  <% end %>
</table>

<%= link_to "Download as CSV", :action => "to_csv", :id => @table_name %> | 
<%= link_to 'Back', :action => "index" %>
