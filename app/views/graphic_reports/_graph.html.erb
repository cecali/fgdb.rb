<% tempfile = `mktemp -p #{File.join(RAILS_ROOT, "tmp", "tmp")}`.chomp %>
<% basename = File.basename(tempfile).sub(".", "$") %>
<% @report.gnuplot_stuff(tempfile, graph_n) %>
<img src="<%= url_for :action => "get_temp_file", :id => basename, :graph_n => graph_n, :conditions => params[:conditions] %>" />
