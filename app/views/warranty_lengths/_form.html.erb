  <p>
    <%= f.label :system_type %><br />
    <%= f.select :system_type, @rt_metadata["Type of Box"].map{|x| [x, x]}, {:include_blank => true} %>
  </p>
  <p>
    <%= f.label :box_source %><br />
    <%= f.select :box_source, @rt_metadata["Box source"].map{|x| [x, x]}, {:include_blank => true} %>
  </p>
  <p>
    <%= f.label :os_type %><br />
    <%= f.select :os_type, WorkOrdersController::OS_OPTIONS.map{|x| [x, x]}, :include_blank => true %>
  </p>
  <p>
    <%= f.label :length_number, "Length" %><br />
    <%= f.text_field :length_number, :value => f.object.length_number %>
    <%= f.select :length_unit, WarrantyLength::UNITS.map{|x| [x.to_s, x.to_s]}, :select => f.object.length_unit %>
  </p>
  <p>
    <%= f.label :effecitve_on %><br />
    <%= calendar_box('warranty_length', 'effective_on') %>
  </p>
  <p>
    <%= f.label :ineffective_on %><br />
    <%= calendar_box('warranty_length', 'ineffective_on') %>
  </p>
