<% {"Workers" => @workers, "Summary" => @types}.each do |t,x| %>
  <table style="border-collapse: collapse;" border="1">
    <tr><th colspan="5"><%= t %></th></tr>
    <tr><th>name</th><th>hours</th><th>pto</th><th>overtime</th><th>holiday</th></tr>
    <% x.each do |y| %>
      <tr><td><%= y[:name] %></td><td><%= y[:hours] %></td><td><%= y[:pto] %></td><td><%= y[:overtime] %></td><td><%= y[:holiday]%></td></tr>
    <% end %>
  </table>
  <br />
<% end %>
