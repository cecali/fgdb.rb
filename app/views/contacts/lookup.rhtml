<% if @contact %>
 <p>You found "<%=h @contact.display_name %>"</p>
<% end %>
<% form_tag( { :controller => '/contacts', :action => 'lookup' }, { :id => 'tasty_test' } ) do %>
  <div class="form-element">
    <%= render :partial => 'search', :locals => {:contact => @contact, :options => {}} %>
  </div>
  <%= submit_tag 'Test' %>
<% end -%>
