<h1>New till_adjustment</h1>

<% form_for(@till_adjustment, {:action => "create"}) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :till_type_id %><br />
    <%= f.text_field :till_type_id %>
  </p>
  <p>
    <%= f.label :till_date %><br />
    <%= f.date_select :till_date %>
  </p>
  <p>
    <%= f.label :adjustment_cents %><br />
    <%= f.text_field :adjustment_cents %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', {:action => "index"} %>
