<h1>Listing <%= @property_type %></h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% for property in @properties %>
  <tr>
    <td><%= property.name %></td>
    <td><%= link_to 'Edit', :action=>(@property_type + "_edit"), :id=>property.id %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to "New #{@property_type.sub(/s$/, "")}", :action=>(@property_type + "_new") %>
